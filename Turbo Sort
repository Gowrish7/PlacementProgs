import java.util.*;
import java.util.Arrays;
class chef{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int m=sc.nextInt();
        int arr[]=new int[m];
        for(int i=0;i<m;i++)
        {
            arr[i]=sc.nextInt();
        }
        Arrays.sort(arr);
        for(int i=0;i<m;i++)
        {
            System.out.println(arr[i]);
        }
    }
}



